{
  "name": "vkcoin-exchange-bot",
  "version": "1.0.0",
  "description": "VK Coin exchange bot",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --exit --timeout 5000",
    "start": "node index.js",
    "db:drop": "node_modules/.bin/sequelize db:drop --url $DATABASE_URL",
    "db:create": "node_modules/.bin/sequelize db:create --url $DATABASE_URL",
    "db:migrate": "node_modules/.bin/sequelize db:migrate --url $DATABASE_URL",
    "db:migrate:undo": "node_modules/.bin/sequelize db:migrate:undo --url $DATABASE_URL",
    "db:migrate:undo:all": "node_modules/.bin/sequelize db:migrate:undo:all --url $DATABASE_URL",
    "db:seed:all": "node_modules/.bin/sequelize db:seed:all --url $DATABASE_URL",
    "db:seed:undo:all": "node_modules/.bin/sequelize db:seed:undo:all --url $DATABASE_URL"
  },
  "keywords": [
    "exchange",
    "bot",
    "vk",
    "vk coin"
  ],
  "author": "Andrey Khataev <khataev@yandex.ru>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "convict": "^4.4.1",
    "express": "^4.16.4",
    "luxon": "^1.11.2",
    "module-alias": "^2.2.2",
    "node-vk-bot-api": "^2.4.8",
    "numeral": "^2.0.6",
    "pg": "^7.14.0",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.0.0"
  },
  "_moduleAliases": {
    "@root": ".",
    "@modules": "modules",
    "@menu-root": "modules/bot-navigation/root"
  }
}
